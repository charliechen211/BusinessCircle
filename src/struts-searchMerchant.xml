<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="searchMerchant" namespace="/merchant" extends="json-default" >
		<action name="searchMerchant" class="com.t.action.SearchMerchantAction" method="searchMerchant">
			<result type="json">
				<param name="root">result</param>
			</result>
		</action>
		
		<action name="searchBasedLocation" class="com.t.action.SearchMerchantAction" method="searchBasedLocation">
			<result type="json">
				<param name="root">result</param>
			</result>
		</action>
		
		<action name="searchUser" class="com.t.action.SearchMerchantAction" method="searchUser">
			<result type="json">
				<param name="root">result</param>
			</result>
		</action>   
		
	
	</package>
</struts>
