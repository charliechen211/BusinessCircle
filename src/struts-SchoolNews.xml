<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="schoolnews" namespace="/schoolnews" extends="json-default" >
		
		<action name="getList" class="com.t.action.SchoolNewsAction" method="getList">
			<result type="json">
				<param name="root">result</param>
			</result>
		</action>
		
		<action name="getContent" class="com.t.action.SchoolNewsAction" method="getContent">
			<result type="json">
				<param name="root">result</param>
			</result>
		</action>

	</package>
</struts>