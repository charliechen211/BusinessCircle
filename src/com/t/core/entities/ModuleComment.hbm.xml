<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.t.core.entities.ModuleComment" table="ModuleComment" catalog="Smart">
		<id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="increment" />
        </id>
        <property name="userId" type="java.lang.Integer">
       		<column name="userId"/>
        </property>
        <property name="objectId" type="java.lang.Integer">
       		<column name="objectId"/>
        </property>
        <property name="comment" type="java.lang.String">
            <column name="comment" length="100" />
        </property>
        <property name="timestamp" type="java.sql.Timestamp">
        	<column name="timestamp"/>
        </property>
        <property name="type" type="java.lang.Integer">
        	<column name="type"/>
        </property>
        <property name="delFlg" type="java.lang.Integer">
        	<column name="delFlg"/>
        </property> 
        
	</class>
</hibernate-mapping>