<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.t.core.entities.Queue" table="queue" catalog="Smart">
		<id name="queueId" type="java.lang.Integer">
            <column name="queueId" />
            <generator class="increment" />
        </id>
        <property name="merchantId" type="java.lang.Integer">
            <column name="merchantId" />
        </property>
        <property name="userId" type="java.lang.Integer">
            <column name="userId" />
        </property>
        <property name="tableType" type="java.lang.String">
            <column name="tableType" />
        </property>
        <property name="orderNum" type="java.lang.Integer">
            <column name="orderNum" />
        </property>
        <property name="status" type="java.lang.String">
            <column name="status" />
        </property>
        <property name="startTime" type="java.sql.Timestamp">
            <column name="startTime" />
        </property>
        <property name="endTime" type="java.sql.Timestamp">
            <column name="endTime" />
        </property>
	</class>
</hibernate-mapping>