<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="parttime" namespace="/parttime" extends="json-default" >
		
		<action name="addjob" class="com.t.action.PartTimeJobAction" method="addJob">
			<result type="json">
				<param name="root">result</param>
			</result>
		</action>
		
		<action name="getList" class="com.t.action.PartTimeJobAction" method="getList">
			<result type="json">
				<param name="root">result</param>
			</result>
		</action>
		
		<action name="getDetails" class="com.t.action.PartTimeJobAction" method="getDetails">
			<result type="json">
				<param name="root">result</param>
			</result>
		</action>
		
	</package>
</struts>