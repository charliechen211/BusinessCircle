<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="interactionReply" namespace="/interactionReply" extends="json-default" >
		
		<action name="addReply" class="com.t.action.InteractionReplyAction" method="addReply">
			<result type="json">
				<param name="root">result</param>
			</result>
		</action>
		
		<action name="getReplyList" class="com.t.action.InteractionReplyAction" method="getReplyList">
			<result type="json">
				<param name="root">result</param>
			</result>
		</action>
 
	</package>
</struts>