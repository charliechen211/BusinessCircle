<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="shlvinfo" namespace="/shlvinfo" extends="json-default" >
		<action name="fetchInfos" class="com.t.action.ShLVInfoAction" method="fetchInfos">
			<result type="json">
				<param name="root">result</param>
			</result>
		</action>
		
		<action name="addInfo" class="com.t.action.ShLVInfoAction" method="addInfo">
			<result type="json">
				<param name="root">result</param>
			</result>
		</action>
		
		<action name="getInfo" class="com.t.action.ShLVInfoAction" method="getInfo">
			<result type="json">
				<param name="root">result</param>
			</result>
		</action>
		
	</package>
</struts>
